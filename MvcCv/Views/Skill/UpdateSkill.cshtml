@model MvcCv.Models.Entity.TblSkills

@{
    ViewBag.Title = "UpdateSkill";
    Layout = "~/Views/Shared/_AdimLayout.cshtml";
}

<br />
<h4>Yeni Yetenek Ekleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Yetenek Düzenleme Sayfası")
    @Html.TextBoxFor(x => x.Skills, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Rate, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>


